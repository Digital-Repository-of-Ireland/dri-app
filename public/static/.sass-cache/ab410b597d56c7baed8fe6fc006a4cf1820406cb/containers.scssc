@linei:@template"Ö,@charset "UTF-8";
/* CSS Document */

.dri_line {
	width: 1024px;
	margin: 0;
	padding: 0px;
}

.size_0 {
	width: 984px;
	float: left;
	margin: 0;
	padding: 0 20px 0 20px;
}
.size_01 {
	margin: 0 20px 20px 0;
	padding: 0;
	width: 228px;
	float: left;
	display: block;
	color: #2F2F2F;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border: 1px solid #E1E1E1;
	position: relative;

}
.size_01.top {
	border-top: 5px solid #E1E1E1;
}

.size_01.last {
	margin: 0 0 20px 0;
}

.size_01.refine {
	float: right;
	box-shadow: 1px 2px 6px  #E3E3E3;
	-moz-border-radius: 10px;
	border-radius: 10px;
}
.size_01 .high_view {
	background: image-url('high_view_icon.png') no-repeat;
	position: absolute;
	left: 185px;
	top: -6px;
	display: block;
	height: 50px;
	width: 32px;
	z-index: 99;
}


/* ----------- Collection block ---------- */
.content_block_wrap {
	width: 730px;
	margin: 0 20px 0 20px;
	float: left;
	padding: 0;
}
.dri_content_block {
	margin: 0 20px 20px 0;
	padding: 0;
	width: 228px;
	//height:270px;
	float: left;
	display: block;
	color: #2F2F2F;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border: 1px solid #E1E1E1;
	position: relative;

}
.dri_content_block.top {
	border-top: 5px solid #E1E1E1;
}
.dri_content_block.last {
	margin: 0 0 20px 0;
}

.dri_content_block .dri_high_view {
	background: image-url('high_view_icon.png') no-repeat;

}

.dri_content_block h1 {
	color: #2F2F2F;
	font-size: 110%;
	padding: 0;
	margin: 10px 14px 20px 14px;
	height: 35px;
	overflow: hidden;
  	text-overflow: ellipsis;

}
.dri_content_block h1 a {
	color:#2F2F2F;
	text-decoration: none;
}
.dri_content_block a:hover h1 {
	text-decoration:underline;

}
.dri_content_block h2 {
	color: #2F2F2F;
	font-size: 100%;
	padding: 0;
	margin: 4px 14px 4px 14px;
	font-weight: normal;
}
.dri_content_block h2 a {
	color: #2F2F2F;
}
.dri_content_block h2 a:hover {
	text-decoration:underline;

}
.dri_content_block h2.dri_editor {
	color: #808080;
	line-height: 1.4em;
	display: block;
	height: 36px;
	white-space: wrap;
  	overflow: hidden;
  	text-overflow: ellipsis;
}

.dri_content_block h2.dri_editor a {
	color: #808080;
}
.dri_content_block h2.dri_editor a:hover {
	text-decoration: none;
}
.dri_content_block p {
	color:#2F2F2F;
	font-size: 100%;
	padding: 0;
	margin: 10px 10px 10px 10px;

}
.dri_image {
	padding: 0;
	margin: 0 14px 0 14px;
}

.dri_content_block .dri_stats {
	color: #808080;
	line-height: 1.4em;
	display: block;
	height: 26px;
	margin: 4px 14px 4px 14px;
	border-top: 1px solid #E1E1E1;
	padding: 4px 0 0 0;
}
.dri_content_block .dri_stats span.dri_object_num {
	color: #fff;
	display: block;
	height: 20px;
	width: 42px;
	padding: 3px 0 0 6px;
	margin: 0 8px 0 0;
	float: left;
	background: image-url('object_back.png') no-repeat;
}
.dri_content_block .stats span.dri_object_num a {
	color: #fff;
}
.dri_content_block .stats span.dri_object_num a:hover {
	text-decoration: underline;
}

.dri_content_block .dri_stats span.dri_view {
	margin: 4px 8px 0 0;
	display: block;
	height: 20px;
	padding: 3px 0 0 0;
	margin: 0 10px 0 0;
	float: left;
}
.dri_content_block .dri_stats span.dri_like {
	margin: 4px 8px 0 0;
	display: block;
	height: 20px;
	padding: 1px 0 0 0;
	margin: 0 10px 0 0;
	float: left;
}
.dri_content_block .dri_stats span.dri_comment{
	margin: 4px 8px 0 0;
	display: block;
	height: 20px;
	padding: 0;
	margin: 0 10px 0 0;
	float: left;
}
/* ----------- Pagination ---------- */
.dri_pagination {
	width: 984px;
	height: 30px;
	margin: 0 0 10px 0;
	padding: 0 20px 0 20px;
	float: left;
}

 ul.dri_paging {
	height: 30px;
	margin: 3px 0 0 0;
	padding: 0 5px 0 0;
	float: right;
	//width: 275px;
}
 ul.dri_paging li {
	float: left;
	list-style: none;
	display: block;
	padding: 0;
}

 ul.dri_paging li a {
	float: left;
	list-style: none;
	display: block;
	padding: 2px 10px 2px 10px;
	color: #FFFFFF;
	background-color: #6D6E71;
	margin-right: 4px;
}
 ul.dri_paging li a:hover {
	 text-decoration: underline;
}
 ul.dri_paging li.prev_arrow a {
	background: image-url('prev_arrow.png') no-repeat center;
	background-color: none;
}
 ul.dri_paging li.next_arrow a {
	background: image-url('next_arrow.png') no-repeat center;
	background-color: none;
}

 ul.dri_paging li.selected a {
	background-color: #B3B3B3;
	color: #FFF;
}

 ul.dri_paging li.next_num a {
	background-color: #2F2F2F;
	color:#FFF;
}
/* ----------- Filter List Block ---------- */


/* ----------- Search Facet Filters ---------- */


.dri_facet_filters_block {

	width: 225px;
	border: 1px solid #E1E1E1;
	//float: right;
	box-shadow: 1px 2px 6px  #E3E3E3;
	-moz-border-radius: 10px;
	border-radius: 10px;


}
.dri_facet_filters_block.top {
	border-top: 5px solid #E1E1E1;
}
.dri_facet_filters_block.last {
	margin: 0 0 20px 0;
}


.dri_facet_filters ul.filter {
	padding: 0;
	margin: 0;
}

.dri_facet_title {
	padding: 0 0 0 15px;
	margin: 0;
}

.dri_facet_title h5:first-child {
	font-size: 110%;
	color: #2F2F2F;
	text-decoration: none;
}

.dri_facet_filters {
    width: 225px;
    //background: grey;
    margin: 10px -10px -20px -10px;

}
.dri_facet_filters .filter li.title {
	color: #2F2F2F;
	font-size: 110%;
	font-weight: bold;
	padding: 12px 15px 12px 15px;
	margin: 0;
	float: left;
	list-style: none;
	text-decoration: none;
	display: block;
	width: 200px;
}
.dri_facet_filters ul.filter li.title span.search_num {
	font-size: 100%;
	padding: 0;
	margin: 0;
	float: right;
	align: top;
}

.dri_search_num {
	font-size: 100%;
	padding: 0;
	margin: 0;
	float: right;
	align: top;
	color: #808080;
}
.dri_facet_filters ul.filter li.title span.search_num a {
	color: #B3B3B3;
}
.dri_facet_filters ul.filter li.title span.search_num a:hover {
	text-decoration: underline;
}
.dri_facet_filters ul.filter_list {
	padding: 0;
	margin: 0;
}
.dri_facet_filters ul.filter_list  li {
	font-size: 106%;
	padding: 0;
	margin: 0;
	text-decoration: none;
	list-style: none;
}

.dri_facet_filters h4:first-child {
	color:#2F2F2F;
	padding: 4px 15px 14px 15px;
	margin: 0;
	border-bottom: 1px solid #E1E1E1;
	width: 100%;
	font-size: 110%;

}

.dri_facet_filters ul.filter_list li a:hover {
	text-decoration: underline;
}

.dri_facet_filters ul.filter_list li a span.filter_arrow_down {
	float: right;
	width: 14px;
	height: 12px;
	display: block;

}
.dri_facet_filters ul.filter_list li a span.filter_arrow_up {
	float: right;
	width: 12px;
	height: 14px;
	display: block;

}
.dri_facet_filters ul.filter_list_sub {
	margin: 0;
	padding: 0;
}

.filter_list_sub ul{
	margin: -40px;
	padding: auto;
}

.dri_facet_filters ul.filter_list_sub li {
	font-size: 100%;
}
.dri_facet_filters ul.filter_list_sub li a {
	color: #808080;
	background-color: #FAFAFA;
}
.dri_facet_filters ul.filter_list_sub li a:hover {
	background-color: #EEEEEE;
	text-decoration: none;
}
.dri_facet_filters ul.filter_list_sub li.on a {
	text-decoration: underline;
	background-color: #EEEEEE;
}

/* ----------- comment blocks ---------- */

.dri_comment_block {
	width: 730px;
	margin: 0 20px 0 20px;
	float: left;
	padding: 0;
	border-top: 1px solid #E1E1E1;
	padding-top: 20px;
	margin-bottom: 20px;
}


.dri_comment_block h1.dri_comments {
	font-size: 120%;
	margin: 0;
	padding: 0;
	color: #2F2F2F;
}
.dri_comment_block  h1.dri_pc_name {
	font-size: 106%;
	margin: 0;
	padding: 0;
	color: #2F2F2F;
	font-weight: normal;
}
.dri_comment_block h1.dri_pc_name span.dri_date {
	color: #B3B3B3;
	margin-left: 10px;
}

.dri_comment_block .dri_comment_box {
	width: 730px;
	height: 120px;
	border: 1px solid #E1E1E1;
	margin: 10px 0 10px 0;
}
.dri_comment_block p.dri_read_comment {
	margin: 4px 0 0 0;
	padding: 0 80px 0 0;
	color: #808080;
	font-weight: normal;
	font-size: 106%;
}

/* ----------- Object View ---------- */

.dri_object_view {
	//width: 730px;
	margin: 0 20px 0 20px;
	float: left;
	padding: 0;
}
.dri_object_view.dri_padding_none {
	//width: 770px;
	float: left;
	margin-left: 0px;
	margin-right: 0px;
}
.dri_object_view.dri_object_title_top {
	padding: 6px 20px 6px 20px;
	float: left;
	margin: 0;
	background-color:#2F2F2F;
	color:#FFF;
	font-size: 120%;
}


.dri_object_view_block  {
	width: 355px;
	padding: 0;
	float: left;
	margin: 30px 0 0 20px;
}
.dri_object_view_block  h1.dri_title {
	padding: 0 0 0 0;
	margin: 0 0 4px 0;
	font-size: 106%;
	font-weight: normal;
	color: #2F2F2F;
}
.dri_object_view_block  p {
	padding: 0 30px 0 0;
	margin: 0 0 15px 0;
	line-height: 1.4;
	color: #808080;
	font-size: 100%;
}
.dri_object_view_block .dri_back_search a {
	padding: 0;
	margin: 0 30px 0 0;
	color: #278DC1;
	font-size: 100%;
	font-weight: bold;
	float: left;
	display: block;
	width: 130px;
}
.dri_object_view_block ul.dri_title_list {
	padding: 0 0 0 15px;
	margin: 0 0 15px 0;
}
.dri_object_view_block  ul.dri_title_list li {
	line-height: 1.4;
	color: #808080;
	font-size: 100%;
	list-style: square;
}

.dri_object_view_block.dri_border_r {
	border-right: 1px solid #E1E1E1;
	//width: 354px;
}
.dri_object_view_block .dri_image_view {
	//width: 355px;
	//height: 420px;
	display: block;
	background-color: #EAEBEB;
}
.dri_object_view_block.dri_last {
	margin-right: 0px;
}
.dri_object_view_block .dri_like_button {
	margin: 0;
	padding: 2px 10px 2px 10px;
	list-style: none;
	text-decoration: none;
	color: #FFF;
	background-color: #808284;
	-moz-border-radius: 3px;
	border-radius: 3px;
	box-shadow: 1px 1px 4px  #888888;
	width: 50px;
	float: left;
	display: block;
	cursor: pointer;
	}
.dri_object_view_block .dri_like_button:hover {
	background-color: #646768;
	}
.dri_object_view_block .dri_like_button span.dri_like {
	background: image-url('like_button.png') no-repeat;
	width: 20px;
	height: 20px;
	display: block;
	float: left;
	margin: 0;

	}
.dri_object_view_block ul.dri_view_collection {
	margin: 6px 0 0 0;
	padding: 0;
	display: block;
	float: left;
	width: 230px;
	border-top: 1px solid #E1E1E1;
}

.dri_object_view_block ul.dri_view_collection li {
	margin: 0;
	padding: 3px 0 3px 0;
	list-style: none;
	border-bottom: 1px solid #E1E1E1;
	color: #808080;
}
.dri_object_view_block ul.dri_view_collection li span.dri_stat_num a {
	float: right;
	font-weight: bold;
	color:#278DC1;
}

.dri_object_view_block ul.dri_view_collection li span.dri_stat_num a:hover {
	text-decoration: underline;
}

.dri_object_view_block ul.dri_stats {
	margin: 20px 0 0 0;
	padding: 4px 0 4px 0;
	width: 230px;
	float: left;
	border-bottom: 1px solid #E1E1E1;
	border-top: 1px solid #E1E1E1;
}

.dri_object_view_block ul.dri_stats li {
	float: left;
	width: 230px;
	display: block;
	margin: 0;
	padding: 0;
	list-style: none;
	color: #808080;
}

.dri_object_view_block ul.dri_stats li span.dri_stat_num {
	float: right;
	font-weight: bold;
}
.dri_object_view_block .dri_bookmark_object {
	margin: 20px 0 0 0;
	padding: 0;
	display: block;
	float: left;
	width: 230px;
	font-weight: bold;
	color: #2F2F2F;
	font-size: 106%;
}
.dri_object_view_block .dri_bookmark_object select {
	margin: 6px 0 0 0;
	padding: 0;	float: left;
	width: 230px;
}

.dri_object_view_block .dri_save_search {
	margin: 20px 0 0 0;
	padding:0;
	display:block;
	float: left;
	width: 300px;

}
.dri_object_view_block .dri_save_search input{
	margin: 0 10px 0 0;
	padding:0;
	float: left;

}

.dri_object_view_block .dri_vc_title {
	margin: 20px 0 0 0;
	padding: 0;
	display: block;
	float: left;
	width: 230px;
	font-weight: bold;
	color: #2F2F2F;
	font-size: 106%;
}

.dri_object_view_block .dri_audio_player {
	padding: 0;
	margin: 0 0 30px 0;
}

.dri_h140 {
	height: 140px;
}

.mb20 {
	margin-bottom: 20px;
}
