/* These styles assume you are using ul and li */
.dropit {
    list-style: none;
	padding: 0;
	margin: 0;

}
.dropit .dropit-trigger {
	position: relative;

}

.dropit .dropit-submenu {
    position: absolute;
    top: 100%;
    z-index: 1000;
    display: none;
	padding: 0px;
	left: -20px;
	top: 27px;
	background-color: #F1F2F2;
    filter: alpha(opacity=85);
    -moz-opacity:0.85;
    opacity: 0.90;
    border-bottom: 1px solid #E1E1E1;
	border-right: 1px solid #E1E1E1;
	border-left: 1px solid #E1E1E1;
	z-index: 99;
}

.dropit ul.dropit-submenu li {
	//list-style: none;
	text-decoration: none;
	font-size: 150%;
	border-bottom: 1px solid #E1E1E1;
	padding: 2px 30px 2px 20px;
}
.dropit .dropit-open .dropit-submenu { display: block; }


/*-------------- end dropit --------------*/

/*-------------- styling for search nav --------------*/

.dri_search_menu {
	width: 984px;
	//height: 30px;
	margin: 20px 0 20px 0;
	padding: 0 20px 0 20px;
	float: left;
}

.dri_search_menu ul.option {
	margin: 0px 20px 0 0;
	padding: 0px;
	float: left;
}

.dri_search_menu ul.option li {
	margin: 0;
	padding: 4px 0 4px 15px;
	list-style: none;
	text-decoration: none;
	color: #FFF;
	background-color: #2F2F2F;
	-moz-border-radius: 3px;
	border-radius: 3px;
	box-shadow: 1px 1px 4px  #888888;


}

.dri_search_menu ul.option  li a {
	margin: 0 0 0 20px;
	padding: 0 10px 0 10px;
	color:#FFF;
	border-left: 1px solid #999;


}

.dri_search_bar {
	width: 228px;
	float: right;
	margin: 0 0 0 0;
	padding: 0;
}


.dri_search_bar input {
	width: 208px;
	float: right;
	margin: 0 0 0 0;
	padding: 6px 10px 6px 10px;
	-moz-border-radius: 15px;
	border-radius: 15px;
	border: 1px solid #E1E1E1;
	color: #808080;
}

.dri_search_menu ul.dri_search_term {
	margin: 0px 20px 0 0;
	padding: 0px;
	float: left;
}


.dri_search_menu ul.dri_search_term  li {
	//margin: 0;
	//padding: 4px 0 4px 15px;
	list-style: none;
	text-decoration: none;
	//color: #FFF;
	//background-color: #2F2F2F;
	//-moz-border-radius: 3px;
	//border-radius: 3px;
	//box-shadow: 1px 1px 4px  #888888;

}

.dri_search_menu ul.dri_search_term  li a {
	margin: 0 0 0 20px;
	padding: 0 10px 0 10px;
	color:#FFF;
	border-left: 1px solid #999;

}

.dri_search_constraint_term {
	margin: 10px 20px 20px 0;
	padding: 4px 0 4px 15px;
	list-style: none;
	text-decoration: none;
	color: #FFF;
	background-color: #2F2F2F;
	-moz-border-radius: 3px;
	border-radius: 3px;
	box-shadow: 1px 1px 4px  #888888;
}
.search_page_holder {
	width: 480px;
	margin: 20px 0 20px 0;
	padding: 0 20px 0 0;
	float: right;
}
.dri_contraint_search {
	width: 500px;
	margin: 20px 0 20px 0;
	padding: 0 0 0 20px;
	float: left;
}

.dri_contraint_search  ul.search_term {
	margin: 0px 6px 0 0;
	padding: 0px;
	float: left;
}

.dri_contraint_search  ul.search_term  li {
	margin: 0 0 8px 0;
	padding: 4px 0 4px 15px;
	list-style: none;
	text-decoration: none;
	color: #FFF;
	background-color: #2F2F2F;
	-moz-border-radius: 3px;
	border-radius: 3px;
	box-shadow: 1px 1px 4px  #888888;
}
.dri_contraint_search  ul.search_term  li a {
	margin: 0 0 0 20px;
	padding: 0 10px 0 10px;
	color:#FFF;
	border-left: 1px solid #999;
}
/* ----------- Pagination ---------- */
.dri_pagination_container {
    width: 275px;
	float: right;
}

.dri_pagination {
	//width: 984px;
	//height: 30px;
	margin: 0 0 10px 0;
	padding: 0 20px 0 20px;
	float: left;
}


 ul.dri_paging {
	//height: 30px;
	//margin: 3px 0 0 0;
	padding: 0;
	//float: right;
	//width: 175px;
}
 ul.dri_paging li {
	float: left;
	list-style: none;
	display: block;
	padding: 0;
}

 ul.dri_paging li a {
	float: left;
	list-style: none;
	display: block;
	padding: 2px 10px 2px 10px;
	color: #FFFFFF;
	background-color: #6D6E71;
	margin-right: 4px;
}
 ul.dri_paging li a:hover {
	 text-decoration: underline;
}
 ul.dri_paging li.dri_prev_arrow a {
	background: image-url('prev_arrow.png') no-repeat center;
	background-color: none;
}
 ul.dri_paging li.dri_next_arrow a {
	background:image-url('next_arrow.png') no-repeat center;
	background-color: none;
}

 ul.dri_paging li.dri_selected a {
	background-color: #B3B3B3;
	color: #FFF;
}

 ul.dri_paging li.dri_next_num a {
	background-color: #2F2F2F;
	color:#FFF;
}

	/* button styling ----------------------------------- */

input.dri_request_button {
	font-size: 100%;
	background-color: #7573C0;
	color: #FFF;
	-moz-border-radius: 3px;
	border-radius: 3px;
	border: none;
	padding: 6px 30px 6px 30px;
	margin: 20px 0 0 0;
	cursor: pointer;
	display:none;
}
input.dri_request_button:hover {
	background-color: #595BAA;
}

	/* footer ----------------------------------- */
dri_footer {
	margin-bottom: 40px;
	display: block;
	height: 50px;
	padding: 20px 0 70px 0;
	margin: 0 auto;
	color: #9a9a9a;
	text-align: right;
	width: 930px;
	}
	footer a {
	color: #9a9a9a;
	text-decoration: none;
	}
	footer a:hover {
	color: #787878;
	}

.base {
	padding: 0;
	margin-bottom: 40px;
	width: 960px;
	margin: auto;
	display: block;
	height: 50px;
	padding: 10px 0 0 0;
	margin-bottom: 100px;
	color: #9a9a9a;
	}

/* Fixes for refactoring */

.dri_non_display_search_button {
	display: none;
	
}

/* Clear Fix ----------------------------------- */
.dri_clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}

.dri_clearfix {
	display: block;
}

html[xmlns] .dri_clearfix {
	display: block;
}

* html .dri_clearfix {
	height: 1%;
}
