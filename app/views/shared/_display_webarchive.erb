<% content_for :head do %>
  <% javascript_include_tag 'replay/ui.js', async: Rails.env.production? %>
<% end %>

<%= content_tag('div', id: "dri_webarchive_view #{id}", data: { url: url, file: generic_file_name}) do %>
  <replay-web-page replayBase='/replay/' source=<%= url %> view="pages"></replay-web-page>
<% end %>