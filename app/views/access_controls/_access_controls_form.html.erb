<div class="dri_ingest_form_container">
	<div class="dri_ingest_form" style="width: 100%">

        <%= form_for @object, as: :batch, url: {controller: "access_controls", action: "update"}, method: "put",
            html: {class: 'form-vertical', id: 'set_access_controls'} do |f| %>

        <%= render partial: "access_controls/access_controls", locals: {f: f, record: @object, inherit_disabled: @object.root_collection? } %>
        <%= f.submit t('dri.views.objects.buttons.save'), class: "hidden" %>

    <% end %>
  </div>
</div>
