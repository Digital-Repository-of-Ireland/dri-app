<% if params[:report].presence == 'user' %>
  <%= render partial: 'user_activity' %>
<% elsif params[:report].presence == 'object' %>
  <%= render partial: 'object_activity' %>
<% elsif params[:report].presence == 'fixity' %>
  <%= render partial: 'fixity_report' %>
<% elsif params[:report].presence == 'stats' %>
  <%= render partial: 'summary' %>
  <%= render partial: 'files_report' %>
  <%= render partial: 'stats_report' %>
<% end %>
